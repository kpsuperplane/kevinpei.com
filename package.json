  "bugs": {
    "url": "https://github.com/kpsuperplane/kevinpei.com/issues"
  },
  "homepage": "https://github.com/kpsuperplane/kevinpei.com#readme",
  "dependencies": {
    "@11ty/eleventy": "^0.11.1",
    "sass": "^1.27.0"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}
